{
	"_version": "1.12.0",
	"sap.app": {
		"id": "nz.co.foodstuffs.apps.business.stocktake.management",
		"type": "application",
		"dataSources": {
			"invoiceRemote": {
			  "uri": "https://services.odata.org/V2/Northwind/Northwind.svc/",
			  "type": "OData",
			  "settings": {
				"odataVersion": "2.0"
			  }
			}
		  }		
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.60.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"rootView": {
			"viewName": "nz.co.foodstuffs.apps.business.stocktake.management.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "nz.co.foodstuffs.apps.business.stocktake.management.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/todoitems.json"
			},
			"invoice": {
				"dataSource": "invoiceRemote"
			  }
		},
		"resources": {
			"css": [
				{
					"uri": "css/styles.css"
				}
			]
		}
	}
}
